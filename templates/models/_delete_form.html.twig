<form method="post" class="d-inline" action="{{ path('app_models_delete', {'id': model.id}) }}"
      onsubmit="return confirm({{ 'Are you sure you want to delete this item?'|trans }});">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ model.id) }}">
    <button class="btn btn-outline-danger">
        <i class="fa fa-trash"></i>&nbsp;{{ "Delete" | trans }}
    </button>
</form>
