{% extends 'base.html.twig' %}

{% block title %}Model{% endblock %}

{% block body %}
    <h1>Model</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ model.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ model.nom }}</td>
            </tr>
            <tr>
                <th>TypeTransport</th>
                <td>{{ model.typeTransport }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ model.etat }}</td>
            </tr>
            <tr>
                <th>Ordre</th>
                <td>{{ model.ordre }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_models_index') }}">back to list</a>

    <a href="{{ path('app_models_edit', {'id': model.id}) }}">edit</a>

    {{ include('models/_delete_form.html.twig') }}
{% endblock %}
