<form method="post" class="d-inline" action="{{ path('app_boite_de_vitesses_delete', {'id': boite_de_vitesse.id}) }}"
      onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ boite_de_vitesse.id) }}">
    <button class="btn btn-outline-danger">
        <i class="fa fa-trash"></i>&nbsp;{{ "actions.delete" | trans }}
    </button>
</form>
